  "name": "sussol-react-table",
  "main": "lib/index.js",
  "version": "1.0.0-beta",
  "license": "MIT",
  "description": "A Sussol themed wrapper and simple API for the blueprintjs table component",
  "repository": {
    "type": "git",
    "url": "https://github.com/sussol/sussol-react-table.git"
  },
  "url": "http://github.com/sussol/sussol-react-table",
  "homepage": "http://sussol.net",
  "scripts": {
    "build": "babel --ignore '**/__tests__' src --out-dir lib",
    "lint": "eslint ./ --ext=\".js,.jsx\" --ignore-pattern __tests__/ --ignore-pattern __mocks__/ --ignore-pattern node_modules/ --ignore-pattern lib/ --ignore-pattern examples/",
    "prepare": "yarn build",
    "pretest": "yarn lint",
    "test": "jest src/__tests__/SussolReactTable.spec.js"
  },
  "babel": {
    "presets": [
      "stage-0",
      "env",
      "react"
    ]
  },
  "jest": {
    "verbose": true
  },
  "dependencies": {
    "@blueprintjs/core": "2.0.0-rc.2",
    "@blueprintjs/table": "2.0.0-rc.2",
    "prop-types": "15.6.1",
    "react-transition-group": "^2.2.1"
  },
  "peerDependencies": {
    "material-ui": "^0.19.0 || ^0.16.0",
    "react": "^16.0.0 || ^15.0.0",
    "react-dom": "^16.0.0 || ^15.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.10.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "jest": "22.4.2",
    "material-ui": "^0.19.4",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-test-renderer": "16.2.0"
  }
}
